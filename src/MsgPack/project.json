{
  "compilationOptions": {
    "keyFile": "../MsgPack.snk"
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "FEATURE_TAP" ],
        "optimize": false
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "FEATURE_TAP" ],
        "optimize": true
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../CommonAssemblyInfo.cs"
  ],
  "compileExclude": [
    "Serialization/ExpressionSerializers/**/*.*",
    "MPContract.cs"
  ],
  "frameworks": {
    "net461": {
      "frameworkAssemblies": {
        "System.Numerics": "4.0.0.0",
        "System.Numerics.Vectors": "4.0.0.0"
      }
    }
  }
}
